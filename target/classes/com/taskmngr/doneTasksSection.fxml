<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="500.0" prefWidth="1000.0" BorderPane.alignment="CENTER"
  xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="com.taskmngr.DoneTasksController">
  <children>
    <TableView fx:id="tasksTable" prefHeight="430.0" prefWidth="1000.0"
      BorderPane.alignment="CENTER">
      <columns>
        <TableColumn fx:id="taskColumn" prefWidth="200.0" text="Zadanie" />
        <TableColumn fx:id="descriptionColumn" prefWidth="200.0" text="Opis" />
        <TableColumn fx:id="workerColumn" prefWidth="195.0" text="Osoba odpowiedzialna" />
        <TableColumn fx:id="startDateColumn" prefWidth="200.0" text="Data rozpoczęcia zadania" />
        <TableColumn fx:id="finishDateColumn" prefWidth="200.0" text="Data zakończenia zadania" />
      </columns>
    </TableView>
  </children>
</VBox>